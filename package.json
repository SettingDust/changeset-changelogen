{
  "name": "changeset-changelogen",
  "version": "0.2.5",
  "description": "Generate Changesets from conventional commits",
  "homepage": "http://github.com/SettingDust/changeset-changelogen",
  "main": "bin/index.js",
  "type": "module",
  "keywords": [
    "changesets",
    "changeset",
    "conventional commits"
  ],
  "files": [
    "/bin"
  ],
  "bin": {
    "changeset-changelogen": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SettingDust/changeset-changelogen.git"
  },
  "bugs": {
    "url": "https://github.com/SettingDust/changeset-changelogen/issues"
  },
  "author": "SettingDust",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc -p tsconfig.json",
    "build": "tsup",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "ci:version": "changeset version && pnpm install --lockfile-only",
    "ci:publish": "changeset publish",
    "prepare": "husky install"
  },
  "dependencies": {
    "@changesets/read": "^0.6.0",
    "@changesets/types": "^6.0.0",
    "@changesets/write": "^0.3.1",
    "@manypkg/get-packages": "^2.2.2",
    "changelogen": "^0.5.5",
    "consola": "^3.2.3",
    "scule": "^1.3.0",
    "tsx": "^4.16.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@jest/globals": "^29.7.0",
    "@swc/jest": "^0.2.36",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  },
  "volta": {
    "node": "18.17.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
